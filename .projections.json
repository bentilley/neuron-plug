{
  "Source/*.cpp": {
    "type": "juce",
    "alternate": ["Source/{}.h"]
  },
  "Source/*.h": {
    "type": "juceheader",
    "alternate": ["Source/{}.cpp"]
  },
  "Source/MidiGenerator/WellNeurons/*.cpp": {
    "type": "model",
    "alternate": ["tests/{}.test.cpp", "Source/MidiGenerator/WellNeurons/{}.hpp"]
  },
  "Source/MidiGenerator/WellNeurons/*.hpp": {
    "type": "modelheader",
    "alternate": ["Source/MidiGenerator/WellNeurons/{}.cpp", "test/{}.test.cpp"]
  },
  "tests/*.test.cpp": {
    "type": "test",
    "alternate": ["Source/MidiGenerator/WellNeurons/{}.hpp"]
  },
  "Source/UI/*.cpp": {
    "type": "ui",
    "alternate": ["Source/MidiGenerator/UI/{}.hpp"]
  },
  "Source/UI/*.hpp": {
    "type": "uiheader",
    "alternate": ["Source/MidiGenerator/UI/{}.cpp"]
  }
}
