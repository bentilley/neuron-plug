{
  "Source/Plugin*.cpp": {
    "type": "juce",
    "alternate": ["Source/{}.h"]
  },
  "Source/Plugin*.h": {
    "type": "juceheader",
    "alternate": ["Source/{}.cpp"]
  },
  "Source/UI/*.cpp": {
    "type": "ui",
    "alternate": ["Source/UI/{}.hpp"]
  },
  "Source/UI/*.hpp": {
    "type": "uiheader",
    "alternate": ["Source/UI/{}.cpp"]
  },
  "Source/*.cpp": {
    "type": "cpp",
    "alternate": ["Source/{}.test.cpp", "Source/{}.hpp"]
  },
  "Source/*.hpp": {
    "type": "hpp",
    "alternate": ["Source/{}.cpp", "Source/{}.test.cpp"]
  },
  "Source/*.test.cpp": {
    "type": "test",
    "alternate": ["Source/{}.hpp"]
  }
}
